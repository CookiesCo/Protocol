
package(default_visibility = ["//visibility:public"])
load("//defs:proto.bzl", "proto", "module")


# Testing
proto(
    name = "Terpenes",
    srcs = ["Terpenes.proto"],
    strip_import_prefix = "/opencannabis")

proto(
    name = "Cannabinoids",
    srcs = ["Cannabinoids.proto"],
    strip_import_prefix = "/opencannabis")

proto(
    name = "TestValue",
    srcs = ["TestValue.proto"],
    deps = ["//opencannabismedia:MediaItem"],
    strip_import_prefix = "/opencannabis")

proto(
    name = "TestResults",
    srcs = ["TestResults.proto"],
    deps = [
        ":TestValue",
        ":Terpenes",
        ":Cannabinoids",
        "//opencannabiscontent:Content",
        "//opencannabistemporal:Instant"],
    strip_import_prefix = "/opencannabis")


module(
    name = "opencannabis.structs.labtesting",
    deps = [":TestValue", ":TestResults", ":Terpenes", ":Cannabinoids"],
    strip_import_prefix = "/opencannabis")
