
package(default_visibility = ["//visibility:public"])
load("//defs:proto.bzl", "proto", "module")


# RFID/LLRP
proto(
    name = "RFID",
    srcs = ["RFID.proto"],
    deps = [
        "//ocp/core:Datamodel"],
    strip_import_prefix = "/ocp")

proto(
    name = "LLRP",
    srcs = ["LLRP.proto"],
    deps = [
        ":RFID",
        "//ocp/temporal:Instant",
        "//ocp/core:Datamodel"],
    strip_import_prefix = "/ocp")



module(
    name = "opencannabis.inventory.rfid",
    deps = [
        ":RFID",
        ":LLRP"],
    strip_import_prefix = "/ocp")
