
package(default_visibility = ["//visibility:public"])
load("//defs:proto.bzl", "proto", "module")


# Testing
proto(
    name = "Terpenes",
    srcs = ["Terpenes.proto"],
    strip_import_prefix = "/ocp")

proto(
    name = "Cannabinoids",
    srcs = ["Cannabinoids.proto"],
    strip_import_prefix = "/ocp")

proto(
    name = "TestValue",
    srcs = ["TestValue.proto"],
    deps = ["//ocp/media:MediaItem"],
    strip_import_prefix = "/ocp")

proto(
    name = "TestResults",
    srcs = ["TestResults.proto"],
    deps = [
        ":TestValue",
        ":Terpenes",
        ":Cannabinoids",
        "//ocp/content:Content",
        "//ocp/temporal:Instant"],
    strip_import_prefix = "/ocp")


module(
    name = "opencannabis.structs.labtesting",
    deps = [":TestValue", ":TestResults", ":Terpenes", ":Cannabinoids"],
    strip_import_prefix = "/ocp")
